#include <stdio.h>
int main()
{
    int i, j, fatorial = 0;
    for (i = 1; i <= 10; i++) {
        if (i % 2 != 0) {
            printf("Fatorial de %d\n", i);
            for (j = 1; j <= i; j++) {
                if (j == 1) {
                    fatorial = j;
                }
                
                fatorial *= j;
            }
            printf("Ã‰ igual a %d\n", fatorial);
        }
    }
    return 0;
}
