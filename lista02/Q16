#include <stdio.h>

int main()
{
    int i, s3, s5 = 0;
    for (i = 0; i <= 200; i++) {
        if (i % 3 == 0 & i < 100) {
            s3 += i;
        }
        if (i % 5 == 0 & 100 <= i < 200) {
            s5 += i;
        }
        
    }
    printf("Somatório dos números divisiveis por 3 no intervalo [0,100]: %d\n", s3);
    printf("Somatório dos números divisiveis por 3 no intervalo [0,100]: %d\n", s5);
    return 0;
}
