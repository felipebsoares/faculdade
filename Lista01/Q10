#include <stdio.h>

int main() {
  float largura,altura,comprimento,volume = 0.0;
  printf("------Calcular volume de uma caixa------\n");
  printf("Digite a altura da caixa: ");
  scanf("%f", &altura);
  printf("Digite a largura da caixa: ");
  scanf("%f", &largura);
  printf("Digite o comprimento da caixa: ");
  scanf("%f", &comprimento);

  printf("O volume da caixa é: %.2f cm³", volume = comprimento * altura * largura);
  return 0;
}
